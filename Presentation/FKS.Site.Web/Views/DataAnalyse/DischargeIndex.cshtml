@Styles.Render("~/contents/jquery.plot/jquery.jqplot.min.css")

<div class="easyui-layout" data-options="fit:true">
    <div data-options="region:'north',border:false,collapsible:false" style="height: 90px;padding: 10px; overflow: hidden; border-bottom-width: 1px;">
        <div style="margin: 5px;" class="form-inline">
            <div class="form-group">
                <label class="control-label">开始时间：</label>
            </div>
            <div class="form-group">
                <input data-role="type" type="text" data-options="required:true, showSeconds:false, value:'<%new Date();%>'" class="form-control input-sm easyui-datebox startTime" style="width:150px" />
            </div>
            <div class="form-group">
                <label class="control-label">截止时间：</label>
            </div>
            <div class="form-group">
                <input data-role="type" type="text" data-options="required:true, showSeconds:false, value:'<%new Date();%>'" class="form-control input-sm easyui-datebox endTime" style="width:150px" />
            </div>
        </div>
        <div style="margin: 5px" class="form-inline">
            <div class="form-group">
                <label class="control-label">设备选择：</label>
            </div>
            <div class="form-group">
                <input data-role="type" type="text" class="form-control input-sm EquipInfo" />
            </div>
            <a href="#"
               data-operation="doSearch"
               iconcls="icon-search"
               class="easyui-linkbutton"
               data-options="plain:true,toggle:false">
                查询
            </a>
        </div>
    </div>
    <div data-options="region:'center',border:false,split:true,title:'曲线图'" style="border-right-width:1px;">
        <div class="form-group">
            <label id="dischargeContent" style="margin-bottom:10px"/>
            <div id="discharge" style="margin-top:10px"/>
        </div>
    </div>
</div>
