@Styles.Render("~/contents/jquery.plot/jquery.jqplot.min.css")

<div class="easyui-layout" data-options="fit:true">
    <div data-options="region:'north',border:false,collapsible:false" style="height: 90px;padding: 10px; overflow: hidden; border-bottom-width: 1px;">
        <div style="margin:5px;" class="form-inline">

            <div class="form-group">
                <label class="control-label">开始时间：</label>
            </div>

            <div class="form-group">
                <input data-role="type" type="text" data-options="required:true,showSeconds:false,value:'6/3/2014 13:12'" class="form-control input-sm easyui-datetimebox startTime" style="width:150px" />
            </div>

            <div class="form-group">
                <label class="control-label">截止时间：</label>
            </div>

            <div class="form-group">
                <input data-role="type" type="text" data-options="required:true,showSeconds:false,value:'7/2/2014 13:12'" class="form-control input-sm easyui-datetimebox endTime" style="width:150px" />
            </div>

            @*<a href="#"
               data-operation="doSearch"
               id="start"
               class="easyui-linkbutton"
               data-options="plain:true,toggle:false">
                查询
            </a>*@
            <div class="form-group">
                <a href="javascript:;" data-operation="doSearch" class="btn btn-primary btn-xs active easyui-linkbutton" role="button">查询</a>
            </div>

        </div>
    </div>
    <div data-options="region:'center',split:true,border:false,title:'数据列表'" style="width: 600px; border-right-width: 1px;"></div>
</div>